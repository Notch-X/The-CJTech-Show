@page "/organizers/view/{id:int}"
@inject HttpClient _client
@inject NavigationManager _navManager
<h3>View Organizers</h3>

<FormComponent organizerModel="organizerModel" Disabled="true" />

@code {
    [Parameter] public int id { get; set; }
    Organizer? organizerModel = new Organizer();

    protected async override Task OnParametersSetAsync()
    {
        organizerModel = await _client.GetFromJsonAsync<Organizer>($"{Endpoints.OrganizersEndpoint}/{id}");
    }
}
